{
	"name": "@elsikora/eslint-config",
	"version": "2.1.3",
	"description": "ESLint configuration vision of ElsiKora",
	"keywords": [
		"eslint",
		"eslint-config",
		"eslint-plugin",
		"typescript",
		"nestjs",
		"decorator",
		"sort",
		"import",
		"package.json",
		"perfectionist",
		"sonarjs",
		"typeorm",
		"unicorn",
		"unused-imports",
		"yml",
		"jsonc",
		"yaml"
	],
	"homepage": "https://github.com/ElsiKora/ESLint-Config#readme",
	"bugs": {
		"url": "https://github.com/ElsiKora/ESLint-Config/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ElsiKora/ESLint-Config"
	},
	"license": "MIT",
	"author": "ElsiKora",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"main": "index.js",
	"files": [
		"index.js",
		"json.js",
		"nest.js",
		"typescript.js",
		"yml.js",
		"react.js"
	],
	"scripts": {
		"build": "npm run prebuild && rollup -c",
		"dev": "rollup -c -w",
		"format": "prettier --write \"**/*.{js,json}\"",
		"lint": "eslint --fix ./src && eslint ./*.json --fix",
		"patch": "changeset",
		"prebuild": "rimraf dist",
		"release": "npm install && npm run build && changeset publish"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.1",
		"@elsikora/eslint-plugin-nestjs-typed": "^2.0.4",
		"@eslint-react/eslint-plugin": "^1.15.2",
		"@eslint/compat": "^1.2.2",
		"@eslint/js": "^9.13.0",
		"@rollup/plugin-typescript": "^11.1.6",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/eslint__js": "^8.42.3",
		"eslint": "^9.13.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-check-file": "^2.8.0",
		"eslint-plugin-jsonc": "^2.16.0",
		"eslint-plugin-n": "^17.12.0",
		"eslint-plugin-ng-module-sort": "^1.3.1",
		"eslint-plugin-package-json": "^0.25.0",
		"eslint-plugin-perfectionist": "^3.9.1",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-regexp": "^2.6.0",
		"eslint-plugin-sonarjs": "^2.0.4",
		"eslint-plugin-tailwindcss": "^3.17.5",
		"eslint-plugin-typeorm-typescript": "^0.4.0",
		"eslint-plugin-unicorn": "^56.0.0",
		"eslint-plugin-unused-imports": "^4.1.4",
		"eslint-plugin-yml": "^1.14.0",
		"prettier": "3.3.3",
		"rimraf": "^6.0.1",
		"rollup": "^4.9.6",
		"tslib": "^2.8.0",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.12.2"
	},
	"peerDependencies": {
		"eslint": "^9.13.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
