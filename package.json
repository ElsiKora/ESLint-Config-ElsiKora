{
	"name": "@elsikora/eslint-config",
	"version": "3.2.3",
	"description": "ESLint configuration vision of ElsiKora",
	"keywords": [
		"eslint",
		"eslint-config",
		"eslint-plugin",
		"typescript",
		"nestjs",
		"decorator",
		"sort",
		"import",
		"package.json",
		"perfectionist",
		"sonarjs",
		"typeorm",
		"unicorn",
		"unused-imports",
		"yml",
		"jsonc",
		"yaml"
	],
	"homepage": "https://github.com/ElsiKora/ESLint-Config#readme",
	"bugs": {
		"url": "https://github.com/ElsiKora/ESLint-Config/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ElsiKora/ESLint-Config"
	},
	"license": "MIT",
	"author": "ElsiKora",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		},
		"./cli": {
			"import": "./dist/esm/cli.js",
			"require": "./dist/cjs/cli.js"
		}
	},
	"main": "index.js",
	"bin": {
		"@elsikora/eslint-config": "./bin/index.js"
	},
	"files": [
		"./dist",
		"./bin"
	],
	"scripts": {
		"build": "npm run prebuild && rollup -c",
		"dev": "rollup -c -w",
		"format": "prettier --check .",
		"lint": "eslint ./src && prettier --check .",
		"patch": "changeset",
		"prebuild": "rimraf dist",
		"release": "npm install && npm run build && changeset publish",
		"lint:fix": "eslint ./src --fix && prettier --write .",
		"format:fix": "prettier --write .",
		"test": "vitest",
		"test:coverage": "vitest --coverage",
		"test:watch": "vitest --watch"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.11",
		"@elsikora/eslint-plugin-nestjs-typed": "^2.0.4",
		"@eslint-react/eslint-plugin": "^1.23.2",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@rollup/plugin-typescript": "^12.1.2",
		"@stylistic/eslint-plugin": "^2.13.0",
		"@types/eslint__js": "^8.42.3",
		"@typescript-eslint/eslint-plugin": "^8.20.0",
		"@typescript-eslint/parser": "^8.20.0",
		"@typescript-eslint/utils": "^8.20.0",
		"@vitest/coverage-v8": "2.1.4",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-check-file": "^2.8.0",
		"eslint-plugin-jsonc": "^2.18.2",
		"eslint-plugin-n": "^17.15.1",
		"eslint-plugin-ng-module-sort": "^1.3.1",
		"eslint-plugin-package-json": "^0.19.0",
		"eslint-plugin-perfectionist": "^4.6.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-regexp": "^2.7.0",
		"eslint-plugin-sonarjs": "^3.0.1",
		"eslint-plugin-tailwindcss": "^3.17.5",
		"eslint-plugin-typeorm-typescript": "^0.5.0",
		"eslint-plugin-unicorn": "^56.0.1",
		"eslint-plugin-yml": "^1.16.0",
		"inquirer": "^12.3.2",
		"memfs": "^4.17.0",
		"picocolors": "^1.1.1",
		"prettier": "^3.4.2",
		"rimraf": "^6.0.1",
		"rollup": "^4.30.1",
		"stylelint": "^16.13.1",
		"stylelint-config-css-modules": "^4.4.0",
		"stylelint-config-rational-order": "^0.1.2",
		"stylelint-config-standard-scss": "^14.0.0",
		"stylelint-order": "^6.0.4",
		"stylelint-prettier": "^5.0.2",
		"tslib": "^2.8.1",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.20.0",
		"vitest": "^3.0.4"
	},
	"peerDependencies": {
		"@clack/prompts": "^0.7.0",
		"inquirer": "^12.0.1",
		"picocolors": "^1.1.1"
	},
	"publishConfig": {
		"access": "public"
	}
}
